{
    "name": "shreejan/dash-arrange",
    "version": "1.0.0",
    "description": "A Filament plugin for customizable dashboard widgets with drag & drop functionality",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shreejan Pandit",
            "email": "shreezanpandit@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Shreejan\\DashArrange\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shreejan\\DashArrange\\DashArrangeServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/shreejanpandit/arragne-dashboard/issues",
        "source": "https://github.com/shreejanpandit/arragne-dashboard"
    },
    "homepage": "https://github.com/shreejanpandit/arragne-dashboard",
    "minimum-stability": "dev",
    "prefer-stable": true
}
