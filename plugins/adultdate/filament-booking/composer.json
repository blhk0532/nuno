{
    "name": "adultdate/filament-booking",
    "description": "adultdate filament-booking",
    "keywords": [
        "adultdate",
        "laravel",
        "filament",
        "filament-booking"
    ],
    "homepage": "https://github.com/adultdate/filament-booking",
    "support": {
        "issues": "https://github.com/adultdate/filament-booking/issues",
        "source": "https://github.com/adultdate/filament-booking"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "adultdate",
            "email": "adultdate@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0",
        "filament/forms": "^4.0",
        "filament/tables": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-settings-plugin": "^4.0",
        "filament/spatie-laravel-tags-plugin": "^4.0",
        "flowframe/laravel-trend": "^0.4.0",
        "illuminate/contracts": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.2",
        "squirephp/countries-en": "^3.3",
        "squirephp/currencies-en": "^3.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Adultdate\\FilamentBooking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adultdate\\FilamentBooking\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adultdate\\FilamentBooking\\FilamentBookingServiceProvider"
            ],
            "aliases": {
                "FilamentBooking": "Adultdate\\FilamentBooking\\Facades\\FilamentBooking"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
