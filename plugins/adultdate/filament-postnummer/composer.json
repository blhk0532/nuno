{
    "name": "adultdate/filament-postnummer",
    "description": "A Filament v4 plugin for managing Swedish postal codes (postnummer) with data from multiple sources (Hitta, Ratsit, Merinfo).",
    "type": "filament-plugin",
    "keywords": [
        "filament",
        "laravel",
        "postal-codes",
        "sweden",
        "postnummer",
        "hitta",
        "ratsit",
        "merinfo"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "adultdate",
            "email": "support@adultdate.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Adultdate\\FilamentPostnummer\\": "src/",
            "Adultdate\\FilamentPostnummer\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adultdate\\FilamentPostnummer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adultdate\\FilamentPostnummer\\FilamentPostnummerServiceProvider",
                "Adultdate\\FilamentPostnummer\\FilamentPostnummerPluginServiceProvider"
            ]
        },
        "filament": {
            "plugins": [
                "Adultdate\\FilamentPostnummer\\FilamentPostnummerPlugin"
            ]
        }
    },
    "scripts": {
        "format": [
            "./vendor/bin/pint"
        ],
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}